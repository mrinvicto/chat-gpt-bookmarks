{
    "manifest_version": 3,
    "name": "Chat GPT Bookmarks",
    "version": "1.0",
    "description": "Add a bookmarks block to the ChatGPT sidebar.",
    "permissions": [],
    "content_scripts": [
      {
        "matches": ["https://chatgpt.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }